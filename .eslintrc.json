{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:jest/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "jest", "testing-library"],
	"rules": {
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "off",
		"testing-library/no-debugging-utils": "warn"
	}
}
